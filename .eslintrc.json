{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "indent": 0,
        "semi": [2, "always"],
        "padded-blocks": 0,
        "no-multiple-empty-lines": 0,
        "no-trailing-spaces": 0,
        "space-before-function-paren": 0
    }
}
