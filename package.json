    "url": "https://github.com/victorcanash/IntensiveBackendOpenBootcamp/issues"
  },
  "homepage": "https://github.com/victorcanash/IntensiveBackendOpenBootcamp#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "concurrently": "^7.1.0",
    "eslint": "^7.32.0",
