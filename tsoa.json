{
    "entryFile": "index.ts",
    "noImplicitAdditionalProperties": "throw-on-extras",
    "spec": {
        "outputDirectory": "public",
        "specVersion": 3,
        "securityDefinitions": {
            "jwt":{
                "type":"apiKey",
                "name":"x-access-token",
                "in":"header"
             }
        }
    },
    "routes": {
        "authenticationModule": "./src/middlewares/verifyToken.middleware.ts"
    }
}
